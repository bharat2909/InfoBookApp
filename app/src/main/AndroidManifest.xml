<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myinfobook">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@drawable/info"
        android:label="@string/app_name"
        android:roundIcon="@drawable/info"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyInfoBook">
        <activity
            android:name=".Activity.SportsActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.MuseumsActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.WondersActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.CountryActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/Theme.MyInfoBook1">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>